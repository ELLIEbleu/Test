                        connectionURL="jdbc:mysql://127.0.0.1:3306/tes?useUnicode=true&amp;characterEncoding=utf8&amp;autoReconnect=true&amp;allowMultiQueries=true"
                        userId="root"
                        password="1234">
        </jdbcConnection>

        <!--<javaTypeResolver >-->
        <!--<property name="forceBigDecimals" value="false" />-->
